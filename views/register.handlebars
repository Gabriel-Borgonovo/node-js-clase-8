 {{> navbar}}
 <div class="container">
    <h1>Registrar Usuario</h1>
   
    <!-- <form class="form" onsubmit="send(event)"> -->
    <form action="/api/usuarios" class="form" onsubmit="send(event)">  
        <div class="blur">
            <input 
                id="form-name"
                type="text" 
                name="form-name"
                placeholder="Nombre"
            />
            <input 
                id="form-lastName"
                type="text" 
                name="form-lastName"
                placeholder="Apellido"
            />
            <input 
                id="form-file"
                type="file" 
                name="form-file"
                placeholder="Foto"
            />
            <input type="submit" value="Guardar Usuario"/>
        </div>
            
    </form>
    <p id="created"></p>
</div>

<script src="./js/register.js"></script>